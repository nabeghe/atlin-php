{
    "name": "nabeghe/atlin",
    "description": "A lightweight, high-performance key-value format parser/serializer with APCu, Redis and file-cache support.",
    "keywords": ["atlin", "parser", "serializer", "translation", "i18n", "key-value", "cache"],
    "type": "library",
    "homepage": "https://github.com/nabeghe/atlin-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Hadi Akbarzadeh",
            "email": "elsiomcoder@gmail.com",
            "homepage": "https://elatel.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nabeghe\\Atlin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nabeghe\\Atlin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
